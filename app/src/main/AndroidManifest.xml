<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pointmarket">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PointMarket">
        <activity
            android:name=".activities.RegUserActivity"
            android:label="@string/title_activity_reg_user"
            android:theme="@style/Theme.PointMarket.NoActionBar"></activity>
        <activity android:name=".activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.EditProductActivity" />
        <activity
            android:name=".activities.MenuActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.PointMarket.NoActionBar"></activity>
        <activity android:name=".activities.RegProductActivity" />
    </application>

</manifest>